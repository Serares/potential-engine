{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "outDir": "../wwwroot/js",
    "rootDir": "./src",
    "sourceMap": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["DOM", "ES2022"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules"]
} 